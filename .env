# ======================

NEXT_PUBLIC_ENVIRONMENT=local

# Algod
NEXT_PUBLIC_ALGO_TOKEN=""
NEXT_PUBLIC_ALGO_SERVER="https://testnet-api.algonode.cloud"
NEXT_PUBLIC_ALGO_PORT="443"
NEXT_PUBLIC_ALGO_NETWORK="testnet"

# Indexer
NEXT_PUBLIC_INDEXER_TOKEN=""
NEXT_PUBLIC_INDEXER_SERVER="https://testnet-idx.algonode.cloud"
NEXT_PUBLIC_INDEXER_PORT=""

